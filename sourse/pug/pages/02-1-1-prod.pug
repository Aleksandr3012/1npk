block title
	title Щебень фракции 5-20


block pagecontent
	+header() 
		+top-nav()(class="fixed-page ")
		//- +header-block()
	+b.main-row.row.no-gutters
		+e.col-aside.col-xl-auto 
			+e.current-nav.toggle-btn-js.d-xl-none
				.container Продукция
			
			+aside-nav



		+e.col-main.col
			+s-card-head('Щебень фракции 5-20','по оптовым ценам', 'от 450 руб/тн с НДС', 1,2,3,4)
				
				.p Прочность: М1000, М1200, М1400
				.p Лещадность: от 8% до 27%
				.p Морозостойкость: до 300 циклов
				.p Плотность щебня: от 1,36 т/м3
				.p Радиоактивность:допустимая


			+s-prod-03(class="section-border-bottom")
			// start s-text
			+b.s-text.section#s-text.lazy-background(class="section-border-bottom")&attributes(attributes)
				.container 
					.row
						+item-text-whithoutIcon('Выгодные условия')
							+e.text Мы&nbsp;всегда готовы предложить выгодные условия для наших клиентов.
						+item-text-whithoutIcon('Неизменность цены')
							+e.text Все непредвиденные расходы мы&nbsp;берем на&nbsp;себя, вы&nbsp;платите только оговоренную сумму.
						+item-text-whithoutIcon('Доступная цена')
							+e.text Мы&nbsp;снизили производственные издержки за&nbsp;счет полноценной логистики и&nbsp;развитию собственных карьеров. Именно поэтому, стоимость наших материалов остается доступной.
						+item-text-whithoutIcon('Программа лояльности (бонусы)')
							+e.text Предоставляем закупщикам дополнительный стимул к&nbsp;организации более эффективных закупок.
			// end s-text

			+s-now-03(class="section-border-bottom")
				.container
					+b.section-title.text-center
						h2 Первая нерудная компания сегодня
						+e.text.text-gray2.fw-400 Основным направлением нашей деятельности является дистрибуция нерудных материалов любых объемов и&nbsp;любыми доступными способами по&nbsp;территории России

			+s-video(class="section-border-bottom")

			//- +s-char(class="section-border-bottom d-none d-md-block")

			+s-applications(class="section-border-bottom d-none d-md-block")
				+b.section-title.text-center
					h2 Применение щебня фракции 5-20
			+s-shipping2(class="section-border-bottom")
				+b.section-title.text-center
					h2 Доставка щебня фракции 5-20

			+s-text(class="section-border-bottom")

			+s-form-03(class="section-border-bottom")
				+b.section-title
					h2 Бесплатно доставим образцы щебня

					+e.text.text-secondary.fw-400 Мы&nbsp;гарантируем качество нашей продукции и&nbsp;отправим 20&nbsp;кг материала для исследований моментально по&nbsp;вашему запросу

			+s-objects-03(class="section-border-bottom")

			+s-geo(class="section-border-bottom bgin")
				+b.section-title.text-center
					h2 География поставок
					+e.text.h55.fw-400.text-secondary Осуществляем поставки от&nbsp;карьера до&nbsp;объекта по&nbsp;Республике Татарстан и&nbsp;Республике Башкортостан

			+s-why(class="section-border-bottom d-none d-md-block")

			+s-calc(class="section-border-bottom")
				+b.section-title.text-center
					h2 Калькулятор поставок
					+e.text.h55.fw-400.text-secondary Узнайте сейчас точную стоимость поставки щебня до&nbsp;вашего объекта

			+s-content(class="section-border-bottom")
